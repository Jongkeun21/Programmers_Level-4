    "def solution(n) :\n",
    "    dic = {2: 3, 4: 11}\n",
    "    \n",
    "    if n in dic :\n",
    "        return dic[n]%1000000007\n",
    "    else :\n",
    "        for i in range(6, n+1, 2) :\n",
    "            dic[i] = dic[i-2]*4 - dic[i-4]\n",
    "    \n",
    "        return dic[n]%1000000007"
   ]
  }
